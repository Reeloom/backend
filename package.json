{
  "name": "targup-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc -p . -w --preserveWatchOutput",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch src src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "build:clean": "rm -rf dist && pnpm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "start:prod": "node dist/server.js",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "studio": "prisma studio",
    "format:schema": "prisma format",
    "docker:db:up": "docker compose -f docker-compose.db.yml up -d",
    "docker:db:down": "docker compose -f docker-compose.db.yml down",
    "docker:backend:up": "docker compose -f docker-compose.db.yml -f docker-compose.backend.yml up --build",
    "docker:backend:down": "docker compose -f docker-compose.backend.yml down",
    "docker:all:up": "docker compose -f docker-compose.db.yml up -d && docker compose -f docker-compose.backend.yml up --build -d",
    "docker:all:down": "docker compose -f docker-compose.backend.yml down && docker compose -f docker-compose.db.yml down",
    "docker:backend:rebuild": "docker compose -f docker-compose.backend.yml -f docker-compose.db.yml up --build -d",
    "docker:db:rebuild": "docker compose -f docker-compose.db.yml up --build -d"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "eventemitter2": "^6.4.9",
    "express": "^5.1.0",
    "googleapis": "^153.0.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "prom-client": "^14.1.1",
    "redis": "^4.6.7",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.0"
  },
  "pnpm": {
    "notes": "Always use pnpm for all package management tasks.",
    "ignoredBuiltDependencies": [
      "@prisma/engines"
    ]
  }
}
